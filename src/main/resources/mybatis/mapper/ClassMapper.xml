<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.havenwithyou.mongnewmong.mapper.ClassMapper">

    <resultMap id="classMap" type="ClassDto">
        <id  property="id"	 column="CLASS_ID" />
        <id  property="name"	 column="CLASS_NAME" />
        <id  property="day"   column="DAY"/>
        <id property="starttime" column="START_TIME"/>
        <id property="endtime" column="END_TIME"/>
        <id property="category" column="CATEGORY"/>
        <id property="teacherid" column="TEACHER_ID"/>
        <id property="centerid" column="CENTER_ID"/>
    </resultMap>


</mapper>